{{#each this.note}}
<form class="comment-wrapper">
  <div class="form-group">
    <input readonly type="text" class="form-control" id="comment-title" placeholder={{this.title}} data-noteId={{this._id}}>
    </input>
  </div>
  <div class="form-group">
    <textarea readOnly class="form-control" rows=1 id="comment" placeholder={{this.comment}} data-noteId={{this._id}}>
    </textarea>
  </div>
  <div class="btn btn-info btn-sm save-note" data-noteId={{this._id}}>
    <i class="fas fa-unlock"></i> Unlock to update</div>
</form>
{{/each}}